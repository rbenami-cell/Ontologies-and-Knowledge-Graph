ex: a owl:Ontology ;
  rdfs:label "Device Ontology" .

ex:Device a owl:Class .
ex:SystemDevice a owl:Class ; rdfs:subClassOf ex:Device .
ex:TransportNode a owl:Class ; rdfs:subClassOf ex:SystemDevice .
ex:IPRouter a owl:Class ; rdfs:subClassOf ex:SystemDevice .
ex:ORANFunction a owl:Class ; rdfs:subClassOf ex:SystemDevice .
ex:O_DU a owl:Class ; rdfs:subClassOf ex:ORANFunction .
ex:O_CU a owl:Class ; rdfs:subClassOf ex:ORANFunction .
ex:UPF a owl:Class ; rdfs:subClassOf ex:SystemDevice .

ex:hasInterface a owl:ObjectProperty ; rdfs:domain ex:Device ; rdfs:range ex:Interface .
ex:Interface a owl:Class .
ex:Port a owl:Class ; rdfs:subClassOf ex:Interface .

ex:Node2 a ex:TransportNode .
ex:Node4 a ex:TransportNode .
ex:R2 a ex:IPRouter .
ex:R4 a ex:IPRouter .
ex:DU_Site2 a ex:O_DU .
ex:CU_Site0 a ex:O_CU .
ex:UPF_Site14 a ex:UPF .

ex:Port2_1 a ex:Port .
ex:Port4_1 a ex:Port .
ex:R2_ge1 a ex:Port .
ex:R4_ge1 a ex:Port .
